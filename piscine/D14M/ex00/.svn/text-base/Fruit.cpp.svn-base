#include "Fruit.h"

Fruit::Fruit()
: name("fruit"), _vitamins(0) {

}

Fruit::Fruit(std::string const name, int vitamins)
: name(name), _vitamins(vitamins) {

}

Fruit::~Fruit() {

}

std::string const & Fruit::getName() const {
	return name;
}

int Fruit::getVitamins() const {
	return _vitamins;
}
